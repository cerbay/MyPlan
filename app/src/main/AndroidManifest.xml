<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="percept.myplan">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activities.BaseActivity" />

        <receiver
            android:name=".receivers.AlarmReceiver"
            android:enabled="true"
            android:exported="false" />

        <service
            android:name=".services.MyIntentService"
            android:exported="false" />

        <activity
            android:name=".Activities.AddContactActivity"
            android:label="@string/title_activity_add_contact"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.AddContactFromPhoneActivity"
            android:label="@string/title_activity_add_contact_from_phone"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SplashActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.LoginActivity_1"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".Activities.SignUpActivity"
            android:label="@string/myplan"
            android:parentActivityName=".Activities.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateHidden">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="percept.myplan.Activities.LoginActivity" />
        </activity>
        <activity
            android:name=".Activities.ForgetPwdActivity"
            android:label="@string/forgetpwdtitle"
            android:parentActivityName=".Activities.LoginActivity_1"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateHidden">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="percept.myplan.Activities.LoginActivity_1" />
        </activity>
        <activity
            android:name=".Activities.QuickMessageDetailsActivity"
            android:label="@string/title_activity_quick_message_details"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.EmergencyContactActivity"
            android:label="@string/title_activity_emergency_contact"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.HelpListActivity"
            android:label="@string/title_activity_help_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.CreateQuickMsgActivity"
            android:label="@string/title_activity_create_quick_msg"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.SendMessageActivity"
            android:label="@string/title_activity_send_message"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.ProfileActivity"
            android:label="@string/title_activity_profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.MoodRatingsActivity"
            android:label="@string/title_activity_moodratings"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.GeoTrackingActivity"
            android:label="@string/title_activity_geotracking"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.ExportMyPlanActivity"
            android:label="@string/title_activity_exportplan"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.NotificationActivity"
            android:label="@string/title_activity_notification"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.AddHopeBoxActivity"
            android:label="@string/title_activity_addhopebox"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="me.crosswall.photo.pick.PickPhotosActiviy"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.SymptomDetailsActivity"
            android:label="@string/title_activity_symptoms"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.StrategyDetailsOwnActivity"
            android:label="@string/title_activity_strategy"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.StrategyDetailsOtherActivity"
            android:label="@string/title_activity_strategy_details_other"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>